{
  "defaultEnv": "prod",
  "prod": {
    "driver": "mysql",
    "host": {
      "ENV": "MYSQL_HOST"
    },
    "port": {
      "ENV": "MYSQL_PORT"
    },
    "database": {
      "ENV": "MYSQL_DATABASE"
    },
    "user": {
      "ENV": "MYSQL_USER"
    },
    "password": {
      "ENV": "MYSQL_PASSWORD"
    },
    "multipleStatements": true
  }
}
